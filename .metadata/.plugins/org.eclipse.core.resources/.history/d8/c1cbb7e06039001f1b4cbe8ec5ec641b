#ifndef ZED_XADC_XADC_H_
#define ZED_XADC_XADC_H_

#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

typedef struct
{
	float temperature;
	float vcc_aux;
	float vcc_bram;
	float vccp_int;
	float vccp_aux;
	float vcco_ddr;
	float vref_p;
	float vref_n;
}xadc_value_type;


int32_t xadcInit(void);
int32_t xadcGetTemperature(float *temperature);
int32_t xadcGetVccInt(float *voltage);
int32_t xadcGetVccAux(float *voltage);
int32_t xadcGetVccBram(float *voltage);
int32_t xadcGetVccpInt(float *voltage);
int32_t xadcGetVccpAux(float *voltage);
int32_t xadcGetVccDdr(float *voltage);

extern xadc_value_type xadc_value;

#endif /* ZED_XADC_XADC_H_ */
